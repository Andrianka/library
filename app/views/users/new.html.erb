<div class="row">
  <div class="span6 offset3">
		<h1>New user</h1>
		<%= form_for(@user) do |f| %>
			<%= render 'user_information', f: f %>
			<%= render 'address_information', f: f %>
			<div class="row">
			  <div class="col col-lg-10 col-offset-2">
			    <%= f.submit "Sign up!", class: "btn btn-primary" %>
			  </div>
			</div>
		<% end %>
		<%= link_to 'Back', users_path %>
	</div>
</div>
